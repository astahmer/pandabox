{
  "name": "@pandabox/prettier-plugin-panda",
  "version": "0.0.1",
  "description": "Prettier plugin for Panda css",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsup --tsconfig tsconfig.build.json --dts",
    "build-fast": "tsup --no-dts",
    "dev": "pnpm build-fast --watch"
  },
  "devDependencies": {
    "@pandacss/types": "^0.32.0",
    "prettier": "3.2.5",
    "vitest": "1.3.0"
  },
  "dependencies": {
    "@pandacss/config": "^0.32.0",
    "@pandacss/core": "^0.32.0",
    "@pandacss/node": "^0.32.0",
    "@pandacss/preset-base": "^0.32.0"
  },
  "homepage": "https://astahmer.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/astahmer/pandabox.git",
    "directory": "packages/prettier-plugin-panda"
  },
  "author": "Alexandre Stahmer",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "pandacss",
    "pandabox",
    "panda",
    "kit",
    "codemod",
    "template-literal",
    "styled2panda",
    "typesafety",
    "typescript"
  ]
}
