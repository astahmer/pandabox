{
  "name": "@pandabox/presets",
  "version": "0.0.1",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./nes.css": {
      "source": "./src/nes.css.ts",
      "types": "./dist/nes.css.d.ts",
      "import": {
        "types": "./dist/nes.css.d.mts",
        "default": "./dist/nes.css.mjs"
      },
      "require": {
        "types": "./dist/nes.css.d.ts",
        "default": "./dist/nes.css.js"
      }
    }
  },
  "scripts": {
    "prebuild": "node ./prepare.mjs",
    "build": "tsup",
    "dev": "tsup --watch src",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "nes.css": "^2.3.0",
    "utopia-core": "^1.3.0"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.33.0",
    "@pandacss/types": "^0.33.0",
    "file-type": "^19.0.0",
    "tsup": "^8.0.2",
    "vitest": "^1.3.1"
  },
  "homepage": "https://astahmer.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/astahmer/pandabox.git",
    "directory": "packages/presets"
  },
  "author": "Alexandre Stahmer",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "src",
    "dist",
    "static"
  ],
  "keywords": [
    "pandacss",
    "pandabox",
    "panda",
    "panda",
    "presets",
    "typesafety",
    "typescript"
  ]
}
